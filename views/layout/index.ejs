<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no" />
    <title>PinPoint</title>
    <link rel="icon" type="image/png" href="/images/logo/logo.png" />
    <meta name="msapplication-TileColor" content="#ffffff" />
    <meta name="msapplication-TileImage" content="/images/favicon/ms-icon-144x144.png" />
    <meta name="theme-color" content="#ffffff" />
    <meta name="viewport" width="device-width" />

    <link rel="stylesheet" href="/css/bootstrap.css" />
    <link href="/css/montserrat.css" rel="stylesheet" />
    <link href="/css/font-awesome.css" rel="stylesheet" />
    <link rel="stylesheet" href="/css/main.css" />
    <link
      href="https://fonts.googleapis.com/css?family=Montserrat:300,400,500,600,700,900|Ubuntu:300,400,500,600,700,900"
      rel="stylesheet"
    />
    <link
      href="//db.onlinewebfonts.com/c/125063e353c1f65c55ba1e3811969217?family=GE+SS+Two+Medium"
      rel="stylesheet"
    />
    <link
      href="//db.onlinewebfonts.com/c/125063e353c1f65c55ba1e3811969217?family=GE+SS+Two+Bold"
      rel="stylesheet"
    />
    <%- include('./partials/scripts.ejs') %>
  </head>
  <body style="direction: <%= __('Direction') %>; font-family: <%= __('FontFamily') %>;">
    <%- include('../pages/'+template) %>
    <script>
      $('.collapse').on('show.bs.collapse', function () {
        $('.navbar-toggler').css('display', 'none');
        $('.homeClass').css('display', 'none');
        $('.navbar-brand').css('display', 'none');
        $('.navbar-close-icon input').css('display', 'grid');
        $('.get-demo-display').css('display', 'none');
        $('.business-insights-display').css('display', 'none');
      });
      $('.navbar-close-icon').click(function () {
        $('.coll').removeClass('show');
        $('.navbar-close-icon input').css('display', 'none');
        $('.navbar-toggler').css('display', 'block');
        $('.navbar-brand').css('display', 'block');
        $('.homeClass').css('display', 'block');
        $('.get-demo-display').css('display', 'block');
        $('.business-insights-display').css('display', 'block');
      });
      if ($('body').css('font-family') == 'GE SS Two Medium') {
        $('.semiBold').css('font-weight', 400);
        $('.bold').css('font-weight', 400);
        $('.homeFont').css('font-weight', 400);
        $('.contactUs').css('font-weight', 400);
        $('.download').css('font-weight', 400);
        $('.semiBoldInsights').css('font-weight', 400);
      }
    </script>
  </body>
</html>
